package com.neumont.csc150.PongModel;

public class Ball {
	private int spawnSide;
	private boolean isTouching;
	private int speed;
	private int X;
	private int Y;
	
	public int getSpawnSide() {
		return spawnSide;
	}
	
	public void setSpawnSide(int spawnSide) {
		this.spawnSide = spawnSide;
	}
	
	public boolean isTouching() {
		return isTouching;
	}
	
	public void setTouching(boolean isTouching) {
		this.isTouching = isTouching;
	}
	
	public int getSpeed() {
		return speed;
	}
	
	public void setSpeed(int speed) {
		this.speed = speed;
	}
	
	public int getX() {
		return X;
	}
	
	public void setX(int x) {
		X = x;
	}
	
	public int getY() {
		return Y;
	}
	
	public void setY(int y) {
		Y = y;
	}

}
