package com.neumont.csc150.PongModel;

public class Paddle {
	private int whichPlayer;
	private int Y;

	public int getWhichPlayer() {
		return whichPlayer;
	}

	public void setWhichPlayer(int whichPlayer) {
		this.whichPlayer = whichPlayer;
	}
	
	public int getY(){
		return Y;
	}
	
	public void setY(int Y){
		this.Y = Y;
	}

}
